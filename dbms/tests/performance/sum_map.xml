<test>
    <type>once</type>

    <stop_conditions>
        <all_of>
            <total_time_ms>30000</total_time_ms>
        </all_of>
    </stop_conditions>

    <settings>
        <max_threads>1</max_threads>
    </settings>

    <substitutions>
        <substitution>
           <name>scale</name>
           <values>
               <value>100000</value>
               <value>1000000</value>
           </values>
       </substitution>
        <substitution>
           <name>func</name>
           <values>
               <value>sumMap</value>
               <value>sumMapWithOverflow</value>
           </values>
       </substitution>
    </substitutions>

    <create_query>
        CREATE TEMPORARY TABLE sum_map_{scale} AS 
        SELECT
            arrayMap(x -> (x % 23), range(50)) AS key,
            arrayMap(x -> intDiv(number, x + 1), range(50)) AS val
        FROM numbers({scale})
    </create_query>

    <query>SELECT {func}(key, val) FROM sum_map_{scale} FORMAT Null</query>
</test>
