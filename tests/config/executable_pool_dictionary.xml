    <structure>
        <id>
            <name>x</name>
        </id>
        <attribute>
            <name>a</name>
            <type>String</type>
            <null_value></null_value>
        </attribute>
        <attribute>
            <name>b</name>
            <type>String</type>
            <null_value></null_value>
        </attribute>
    </structure>

    <source>
        <executable_pool>
