    <structure>
        <key>
            <attribute>
                <name>x</name>
                <type>String</type>
            </attribute>
            <attribute>
                <name>y</name>
                <type>String</type>
            </attribute>
        </key>
        <attribute>
            <name>a</name>
            <type>String</type>
            <null_value></null_value>
