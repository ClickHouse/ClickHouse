<clickhouse>
    <storage_configuration>
        <disks>
            <s3>
                <type>s3</type>
                <endpoint>http://localhost:11111/test/test/</endpoint>
                <access_key_id>clickhouse</access_key_id>
                <secret_access_key>clickhouse</secret_access_key>
            </s3>
            <cached_s3>
                <type>cache</type>
                <max_size>1Gi</max_size>
                <path>cached_s3/</path>
                <disk>s3</disk>
            </cached_s3>
            <cached_s3_encrypted>
                <type>encrypted</type>
                <disk>cached_s3</disk>
                <key>1234567812345678</key>
                <path>cached_s3_encrypted/</path>
            </cached_s3_encrypted>
        </disks>
        <policies>
            <s3>
                <volumes>
                    <main><disk>cached_s3</disk></main>
                </volumes>
            </s3>
            <s3_encrypted>
                <volumes>
                    <main><disk>cached_s3_encrypted</disk></main>
                </volumes>
            </s3_encrypted>
        </policies>
    </storage_configuration>
</clickhouse>
