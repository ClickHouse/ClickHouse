<clickhouse>
    <synonyms_extensions>
        <extension>
            <name>en</name>
            <type>plain</type>
            <path>/etc/clickhouse-server/config.d/ext-en.txt</path>
        </extension>
        <extension>
            <name>ru</name>
            <type>plain</type>
            <path>/etc/clickhouse-server/config.d/ext-ru.txt</path>
        </extension>
    </synonyms_extensions>

    <lemmatizers>
        <lemmatizer>
            <lang>en</lang>
            <path>/etc/clickhouse-server/config.d/lem-en.bin</path>
        </lemmatizer>
    </lemmatizers>
</clickhouse>
