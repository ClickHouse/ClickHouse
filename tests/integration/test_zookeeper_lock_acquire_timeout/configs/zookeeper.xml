<clickhouse>
    <zookeeper>
        <node index="1">
            <host>zoo1</host>
            <port>2181</port>
        </node>
        <session_timeout_ms>10000</session_timeout_ms>
        <!-- long timeouts to ensure that startNewSession() blocks for a while -->
        <operation_timeout_ms>5000</operation_timeout_ms>
        <connection_timeout_ms>5000</connection_timeout_ms>
    </zookeeper>
</clickhouse>
