<clickhouse>
    <ldap_servers>
        <openldap>
            <host>openldap</host>
            <port>389</port>
            <bind_dn>cn={user_name},ou=users,dc=company,dc=com</bind_dn>
            <enable_tls>no</enable_tls>
            <tls_require_cert>never</tls_require_cert>
        </openldap>
    </ldap_servers>
</clickhouse>
