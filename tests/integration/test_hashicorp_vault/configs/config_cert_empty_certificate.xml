<clickhouse>
    <hashicorp_vault>
      <url>https://hashicorpvault:8210</url>
      <cert>
      	<name>client</name>
      </cert>
      <ssl>
        <privateKeyFile>/etc/clickhouse-server/config.d/client.key</privateKeyFile>
        <certificateFile></certificateFile>
        <caLocation>/etc/clickhouse-server/config.d/ca.crt</caLocation>
      </ssl>
    </hashicorp_vault>
</clickhouse>
