<?xml version="1.0" encoding="utf-8"?>

<clickhouse>
    <profiles>
        <default>
            <s3_check_objects_after_upload>1</s3_check_objects_after_upload>
            <enable_s3_requests_logging>1</enable_s3_requests_logging>
            <s3_connect_timeout_ms>10000</s3_connect_timeout_ms>
        </default>
    </profiles>
    <users>
        <default>
            <password></password>
            <networks incl="networks" replace="replace">
                <ip>::/0</ip>
            </networks>
            <profile>default</profile>
            <quota>default</quota>
        </default>
    </users>

    <quotas><default></default></quotas>

    <!-- increase for concurrency test -->
    <background_schedule_pool_size>512</background_schedule_pool_size>
    <background_pool_size>16</background_pool_size>
    <max_thread_pool_size>10000</max_thread_pool_size>
    <thread_pool_queue_size>10000</thread_pool_queue_size>

</clickhouse>
