<clickhouse>
   <dictionary>
       <name>postgres_odbc_hashed</name>
       <source>
           <odbc>
               <table>clickhouse.test_table</table>
               <connection_string>DSN=postgresql_odbc</connection_string>
               <db>postgres</db>
           </odbc>
       </source>
       <lifetime>
           <min>5</min>
           <max>5</max>
       </lifetime>
       <layout>
           <hashed />
       </layout>

       <structure>
           <id>
               <name>id</name>
           </id>

           <attribute>
               <name>column1</name>
               <type>Int64</type>
               <null_value>1</null_value>
           </attribute>

           <attribute>
               <name>column2</name>
               <type>String</type>
               <null_value>''</null_value>
           </attribute>

       </structure>
   </dictionary>
</clickhouse>
