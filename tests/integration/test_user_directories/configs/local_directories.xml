<yandex>
    <user_directories replace="replace">
        <users_xml>
            <path>/etc/clickhouse-server/users3.xml</path>
        </users_xml>
        <local_directory>
            <path>/var/lib/clickhouse/access3/</path>
        </local_directory>
        <local_directory>
            <name>local directory (ro)</name>
            <readonly>true</readonly>
            <path>/var/lib/clickhouse/access3-ro/</path>
        </local_directory>
    </user_directories>
    <users_config remove="remove"/>
    <access_control_path remove="remove"/>
</yandex>
