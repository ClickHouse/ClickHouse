<clickhouse>
    <storage_configuration>
        <disks>
            <blob_storage_disk>
                <type>blob_storage</type>
                <endpoint>?</endpoint>
                <skip_access_check>true</skip_access_check>
            </blob_storage_disk>
        </disks>
        <policies>
            <blob_storage_policy>
                <volumes>
                    <main>
                        <disk>blob_storage_disk</disk>
                    </main>
                </volumes>
            </blob_storage_policy>
        </policies>
    </storage_configuration>
</clickhouse>
