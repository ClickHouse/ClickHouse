            <default>
                <volumes>
                    <main>
                        <disk>s3</disk>
                    </main>
                </volumes>
            </default>
        </policies>
    </storage_configuration>
    <merge_tree>
        <allow_object_storage_vfs>1</allow_object_storage_vfs>
    </merge_tree>
</clickhouse>
