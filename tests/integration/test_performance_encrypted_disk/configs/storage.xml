<clickhouse>
    <storage_configuration>
        <disks>
            <disk_local>
                <type>local</type>
                <path>/var/lib/clickhouse/encdsk/</path>
            </disk_local>
            <disk_aes_128_ctr>
                <type>encrypted</type>
                <disk>disk_local</disk>
                <path>aes_128_ctr/</path>
                <algorithm>aes_128_ctr</algorithm>
                <key>0123456789abcdef</key>
            </disk_aes_128_ctr>
            <disk_aes_192_ctr>
                <type>encrypted</type>
                <disk>disk_local</disk>
                <path>aes_192_ctr/</path>
                <algorithm>aes_192_ctr</algorithm>
                <key_hex>109105c600c12066f82f1a4dbb41a08e4A4348C8387ADB6A</key_hex>
            </disk_aes_192_ctr>
            <disk_aes_256_ctr>
                <type>encrypted</type>
                <disk>disk_local</disk>
                <path>aes_256_ctr/</path>
                <algorithm>aes_256_ctr</algorithm>
                <key>0123456789abcdef0123456789abcdef</key>
            </disk_aes_256_ctr>
        </disks>
        <policies>
            <local_policy>
                <volumes>
                    <main>
                        <disk>disk_local</disk>
                    </main>
                </volumes>
            </local_policy>
            <policy_aes_128_ctr>
                <volumes>
                    <main>
                        <disk>disk_aes_128_ctr</disk>
                    </main>
                </volumes>
            </policy_aes_128_ctr>
            <policy_aes_192_ctr>
                <volumes>
                    <main>
                        <disk>disk_aes_192_ctr</disk>
                    </main>
                </volumes>
            </policy_aes_192_ctr>
            <policy_aes_256_ctr>
                <volumes>
                    <main>
                        <disk>disk_aes_256_ctr</disk>
                    </main>
                </volumes>
            </policy_aes_256_ctr>
         </policies>
    </storage_configuration>
</clickhouse>
