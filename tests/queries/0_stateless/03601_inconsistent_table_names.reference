SELECT 1
FROM v0
CROSS JOIN
(
    SELECT 1
    FROM system.one
) AS y
CROSS JOIN
(
    SELECT 1 AS x
    FROM system.one
) AS x
WHERE x.x = 1
WITH rhs AS
    (
        SELECT 1
        FROM system.one
    )
SELECT lhs.d2
FROM view(
    SELECT dummy AS d2
    FROM system.one
    ALL INNER JOIN
    (
        SELECT *
        FROM view(
            SELECT dummy AS d2
            FROM system.one
        )
    ) AS a ON a.d2 = d2
) AS lhs
ALL RIGHT JOIN rhs USING (d1)
