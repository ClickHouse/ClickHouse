CREATE MATERIALIZED VIEW test_00751.t_mv_00751
(
    `date` Date,
    `platform` Enum8('a' = 0, 'b' = 1),
    `app` Enum8('a' = 0, 'b' = 1)
)
ENGINE = MergeTree
ORDER BY date
SETTINGS index_granularity = 8192 AS
SELECT
    date,
    platform,
    app
FROM test_00751.t_00751
WHERE (app = 
(
    SELECT min(app)
    FROM test_00751.u_00751
)) AND (platform = 
(
    SELECT 
        (
            SELECT min(platform)
            FROM test_00751.v_00751
        )
))
2000-01-01	a	a
2000-01-02	b	b
2000-01-03	a	a
2000-01-04	b	b
2000-01-02	b	b
2000-01-03	a	a
