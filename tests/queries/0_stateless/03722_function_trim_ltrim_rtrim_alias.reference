SELECT TRIM(LEADING '$' FROM '$$both$$'), trimLeft('$$both$$', '$');
both$$	both$$
SELECT TRIM(TRAILING '$' FROM '$$both$$'), trimRight('$$both$$', '$');
$$both	$$both
