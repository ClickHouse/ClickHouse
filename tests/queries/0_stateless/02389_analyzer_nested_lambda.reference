WITH x -> toString(id) AS lambda SELECT arrayMap(x -> arrayMap(y -> concat(lambda(x), '_', lambda(y)), [1,2,3]), [1,2,3]) FROM test_table;
[['0_0','0_0','0_0'],['0_0','0_0','0_0'],['0_0','0_0','0_0']]
