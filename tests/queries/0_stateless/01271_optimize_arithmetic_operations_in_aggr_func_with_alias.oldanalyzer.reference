SELECT min(n AS a) + (1 AS b) AS c
FROM
(
    SELECT number AS n
    FROM numbers(10)
    WHERE (1 > 0) AND ((number AS n) > 0)
)
WHERE ((n AS a) > 0) AND ((1 AS b) > 0)
HAVING ((min(n AS a) + (1 AS b)) AS c) > 0
2
SELECT min(n) + 1 AS c
FROM
(
    SELECT number AS n
    FROM numbers(10)
    WHERE ((number AS n) + 1) > 0
)
WHERE ((n + 1) AS a) > 0
HAVING ((min(n) + 1) AS c) > 0
1
SELECT min(n) + 1 AS c
FROM
(
    SELECT number AS n
    FROM numbers(10)
)
HAVING ((min(n) + 1) AS c) > 0
1
