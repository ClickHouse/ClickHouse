CREATE TABLE default.alters_stats\n(\n    `a` Int32,\n    `b` Int32,\n    `c` Int32,\n    `d` Int32,\n    `k` Int32,\n    `heavy` String,\n    `heavy2` String\n)\nENGINE = MergeTree\nORDER BY a\nSETTINGS experimantal_stats_update_period = 100000, index_granularity = 8192
