SELECT
    a,
    b,
    c,
    d,
    heavy,
    heavy2
FROM alters_stats
PREWHERE (b = 100) AND (c = 0) AND (d = 100)
WHERE ((b = 100) AND (c = 0) AND (d = 100)) AND (a = 10)
