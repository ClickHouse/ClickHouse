<?xml version="1.0"?>
<clickhouse>
    <logger>
        <level>trace</level>
        <console>true</console>
    </logger>

    <tcp_port>9000</tcp_port>
    
    <path>test_02311</path>

    <allow_zookeeper_write>true</allow_zookeeper_write>
    
    <zookeeper> 
        <node>
	    <host>localhost</host>
	    <port>2181</port>
	</node>
    </zookeeper>

    <mark_cache_size>0</mark_cache_size>

    <users>
        <default>
            <password></password>

            <networks>
                <ip>::/0</ip>
            </networks>

            <profile>default</profile>
            <quota>default</quota>
            <access_management>1</access_management>
        </default>
    </users>

    <profiles>
        <default/>
    </profiles>

    <quotas>
        <default />
    </quotas>
</clickhouse>
