SELECT any(number) + (any(number) * 2)
FROM numbers(1, 2)
3
SELECT anyLast(number) + (anyLast(number) * 2)
FROM numbers(1, 2)
6
WITH any(number) * 3 AS x
SELECT x
FROM numbers(1, 2)
3
SELECT 
    anyLast(number) * 3 AS x,
    x
FROM numbers(1, 2)
6	6
SELECT any(number + (number * 2))
FROM numbers(1, 2)
3
SELECT anyLast(number + (number * 2))
FROM numbers(1, 2)
6
WITH any(number * 3) AS x
SELECT x
FROM numbers(1, 2)
3
SELECT 
    anyLast(number * 3) AS x,
    x
FROM numbers(1, 2)
6	6
