SELECT
    k,
    r.k,
    name
FROM n
ALL INNER JOIN r ON k = r.k
WHERE k = r.k AND name = \'A\'
SELECT
    k,
    r.k,
    name
FROM n
ALL INNER JOIN r ON k = r.k
WHERE k = r.k AND like(name, \'A%\')
SELECT
    k,
    r.k,
    name
FROM n
ALL INNER JOIN r ON k = r.k
WHERE k = r.k AND notLike(name, \'A%\')
