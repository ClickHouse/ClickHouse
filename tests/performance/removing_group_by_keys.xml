<test>
    <preconditions>
        <table_exists>hits_10m_single</table_exists>
        <table_exists>hits_100m_single</table_exists>
    </preconditions>

    <substitutions>
        <substitution>
            <name>table</name>
            <values>
                <value>hits_10m_single</value>
                <value>hits_100m_single</value>
            </values>
        </substitution>
    </substitutions>

    <query>SELECT avg(length(URL)) FROM {table} GROUP BY WatchID, CounterID FORMAT Null</query>
    <query>SELECT avg(length(URL)) FROM {table} GROUP BY WatchID, CounterID, WatchID - CounterID FORMAT Null</query>
</test>
