<yandex>
    <logger>
        <console>true</console>
    </logger>
    <text_log remove="remove">
        <table remove="remove"/>
    </text_log>
    <metric_log remove="remove">
        <table remove="remove"/>
    </metric_log>
    <use_uncompressed_cache>0</use_uncompressed_cache>
